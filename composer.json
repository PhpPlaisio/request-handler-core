  "require": {
    "php": ">=7.1.0",
    "setbased/abc": "*",
    "setbased/abc-exception": "^1.0.0",
    "setbased/abc-http-header": "2.0.0",
    "setbased/abc-page": "*",
    "setbased/abc-request-handler": "*",
    "setbased/php-stratum": "*"
  },
  "require-dev": {
    "phing/phing": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
